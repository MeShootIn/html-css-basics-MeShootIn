{
  "name": "html-css-basics-MeShootIn",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/MeShootIn/html-css-basics-MeShootIn.git",
  "author": "MeShootIn <dmitriimishutin@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "live-server --port=0 --host=0.0.0.0",
    "scss": "sass --no-error-css --stop-on-error --style=compressed styles/scss:styles/css",
    "lint": "npm run check:editorconfig && npm run lint:prettier",
    "lint:prettier": "prettier --write .",
    "check": "npm run check:editorconfig && npm run check:prettier",
    "check:editorconfig": "editorconfig-checker",
    "check:prettier": "prettier --check ."
  },
  "devDependencies": {
    "editorconfig-checker": "^5.0.1",
    "live-server": "^1.2.2",
    "prettier": "2.8.4",
    "sass": "^1.56.1"
  },
  "packageManager": "yarn@1.22.19"
}
